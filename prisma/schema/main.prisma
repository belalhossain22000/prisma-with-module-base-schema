

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextSearch", "prismaSchemaFolder"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// role
enum UserRole {
  SUPER_ADMIN
  ADMIN
  AUTHOR
  MODERATOR
}

// user status
enum UserStatus {
  ACTIVE
  BLOCKED
  DELETED
}

// visibility
enum Visibility {
  PUBLIC
  PRIVATE
}

// gender
enum Gender {
  MALE
  FEMALE
}
